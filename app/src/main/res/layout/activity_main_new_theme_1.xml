<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/relativeLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background_main_new_theme_1"
    android:keepScreenOn="true"
    tools:context=".MainActivity">

    <LinearLayout
        android:id="@+id/activity_main_new_theme_1_linear_layout_date"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="145dp"
        android:gravity="center_vertical"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="160dp"
            android:fontFamily="@font/alhurra"
            android:text="الأربعاء"
            android:textColor="@color/white"
            android:textSize="38sp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="40dp"
            android:fontFamily="@font/century_gothic_bold"
            android:text="12:25"
            android:textColor="@color/white"
            android:textSize="75sp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:fontFamily="@font/century_gothic_bold"
            android:text="31"
            android:textColor="#f9d79b"
            android:textSize="40sp" />


    </LinearLayout>

    <LinearLayout
        android:id="@+id/activity_main_new_theme_1_linear_layout_iqama"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="36dp"
        android:paddingStart="90dp"
        app:layout_constraintTop_toBottomOf="@+id/activity_main_new_theme_1_linear_layout_date">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/century_gothic_bold"
            android:text="فبراير 2019 م"
            android:textColor="#4e4e58"
            android:textSize="35sp"
            android:textStyle="bold" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="31dp"
            android:text="30"
            android:textColor="@color/white"
            android:textSize="35sp"
            android:textStyle="bold" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="65dp"
            android:fontFamily="@font/century_gothic_bold"
            android:text="ذو القعدة 1440 ه"
            android:textColor="#4e4e58"
            android:textSize="35sp"
            android:textStyle="bold" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="25dp"
            android:text="30"
            android:textColor="@color/white"
            android:textSize="35sp"
            android:textStyle="bold" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/activity_main_new_theme_1_linear_layout_times_iqama"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:paddingStart="70dp"
        android:paddingTop="120dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/activity_main_new_theme_1_linear_layout_iqama">

        <TextView
            android:id="@+id/activity_main_new_theme_1_text_view_times_iqamah_fajr"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/century_gothic_bold"
            android:text="12:25"
            android:textColor="@color/white"
            android:textSize="55sp" />

        <TextView
            android:id="@+id/activity_main_new_theme_1_text_view_times_iqamah_sunrise"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="6dp"
            android:fontFamily="@font/century_gothic_bold"
            android:text="12:25"
            android:textColor="@color/white"
            android:textSize="55sp" />

        <TextView
            android:id="@+id/activity_main_new_theme_1_text_view_times_iqamah_dhuhr"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:fontFamily="@font/century_gothic_bold"
            android:text="12:25"
            android:textColor="@color/white"
            android:textSize="55sp" />

        <TextView
            android:id="@+id/activity_main_new_theme_1_text_view_times_iqamah_asr"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/century_gothic_bold"
            android:text="12:25"
            android:textColor="@color/white"
            android:textSize="55sp" />

        <TextView
            android:id="@+id/activity_main_new_theme_1_text_view_times_iqamah_maghrib"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/century_gothic_bold"
            android:text="12:25"
            android:textColor="@color/white"
            android:textSize="55sp" />

        <TextView
            android:id="@+id/activity_main_new_theme_1_text_view_times_iqamah_isha"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/century_gothic_bold"
            android:text="12:25"
            android:textColor="@color/white"
            android:textSize="55sp" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/activity_main_new_theme_1_linear_layout_times_azan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:paddingTop="120dp"
        android:paddingEnd="70dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/activity_main_new_theme_1_linear_layout_iqama">

        <TextView
            android:id="@+id/activity_main_new_theme_1_text_view_times_azan_fajr"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/century_gothic_bold"
            android:text="12:25"
            android:textColor="@color/white"
            android:textSize="55sp" />

        <TextView
            android:id="@+id/activity_main_new_theme_1_text_view_times_azan_sunrise"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="6dp"
            android:fontFamily="@font/century_gothic_bold"
            android:text="12:25"
            android:textColor="@color/white"
            android:textSize="55sp" />

        <TextView
            android:id="@+id/activity_main_new_theme_1_text_view_times_azan_dhuhr"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:fontFamily="@font/century_gothic_bold"
            android:text="12:25"
            android:textColor="@color/white"
            android:textSize="55sp" />

        <TextView
            android:id="@+id/activity_main_new_theme_1_text_view_times_azan_asr"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/century_gothic_bold"
            android:text="12:25"
            android:textColor="@color/white"
            android:textSize="55sp" />

        <TextView
            android:id="@+id/activity_main_new_theme_1_text_view_times_azan_maghrib"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/century_gothic_bold"
            android:text="12:25"
            android:textColor="@color/white"
            android:textSize="55sp" />

        <TextView
            android:id="@+id/activity_main_new_theme_1_text_view_times_azan_isha"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/century_gothic_bold"
            android:text="12:25"
            android:textColor="@color/white"
            android:textSize="55sp" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/activity_main_new_theme_1_linear_layout_times_iqama">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="70dp"
            android:layout_marginTop="95dp"
            android:fontFamily="@font/century_gothic_bold"
            android:text="45"
            android:textColor="@color/white"
            android:textSize="45sp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="70dp"
            android:fontFamily="@font/century_gothic_bold"
            android:text="45"
            android:textColor="@color/white"
            android:textSize="45sp" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="145dp"
        android:layout_marginTop="110dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/activity_main_new_theme_1_linear_layout_times_azan">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="40dp"
            android:fontFamily="@font/century_gothic_bold"
            android:text="25"
            android:textColor="@color/white"
            android:textSize="90sp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:fontFamily="@font/century_gothic_bold"
            android:text="31"
            android:textColor="#f9d79b"
            android:textSize="50sp" />

    </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>